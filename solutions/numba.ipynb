   "cell_type": "markdown",
   "id": "cda4f6f8-f954-4969-9234-9ced15d80bbe",
   "metadata": {},
   "source": [
    "Mean squared error:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 96,
   "id": "de521a63-4be8-417b-890d-00a6e2040041",
   "metadata": {},
   "outputs": [],
   "source": [
    "@numba.njit(parallel=True)\n",
    "def mse_nm(x, y):\n",
    "    n = x.shape[0]\n",
    "    s = 0\n",
    "    for i in numba.prange(n):\n",
    "        δ = x[i]-y[i]\n",
    "        s += δ*δ\n",
    "    return s / n\n",
    "mse_nm(x, y); # burn-in"
   ]
  }
 ],
